{
    "version": 2,
    "scope": "ahrota",
    "trailingSlash": false,
    "cleanUrls": true,
    "redirects": [
        { "source": "/(.*)/index.html(/*)", "destination": "/$1" },
        { "source": "/(.*).html(/*)", "destination": "/$1" },
        { "source": "/(.*)(/+)", "destination": "/$1" }
    ],
    "headers": [
        {
            "source": "/(.*).(js|json)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        }
    ]
}
