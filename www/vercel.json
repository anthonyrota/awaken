{
    "version": 2,
    "scope": "ahrota",
    "routes": [
        {
            "src": "/(.*)/",
            "status": 308,
            "headers": {
                "Location": "/$1"
            }
        },
        {
            "src": "/(.*).html",
            "status": 404,
            "dest": "/_404/index.html"
        },
        {
            "src": "/(.*)([^/]+).([^/]+).(js|json|png|svg)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/_404/index.html"
        }
    ]
}
