{
    "name": "www",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build:docs": "cd .. && npm install && npm run build && cd www && npm run build:docs-analyze",
        "build:docs-analyze": "npx -s sh ts-node --project ./tsconfig.ts-node.json ./script/docs/apigen/index.ts",
        "build:template": "ncp src template --stopOnErr && npx -s sh ts-node --project ./tsconfig.ts-node.json ./script/template/index.tsx",
        "build:public": "npm run build:template && del-cli '_files/public/**/*' '!_files/public/pages.*.json' && parcel build 'template/**/*.html' --dist-dir _files/public --detailed-report --no-source-maps && rimraf template && npm run build:public-compressed",
        "build:public-compressed": "rimraf _files/public-br && npx -s sh ts-node --project ./tsconfig.ts-node.json ./script/compressPublic/index.ts",
        "vercel-build": "rimraf public && npm run build:docs && npm run build:public && mkdir public && echo > public/index.html",
        "dev": "node ./script/dev/index.js"
    },
    "dependencies": {
        "@hapi/accept": "5.0.1",
        "@vercel/node": "1.8.1",
        "preact": "10.4.8",
        "preact-router": "3.2.1",
        "tslib": "2.0.1"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.10.4",
        "@babel/plugin-transform-react-jsx": "7.10.4",
        "@babel/preset-env": "7.11.5",
        "@babel/preset-typescript": "7.10.4",
        "@microsoft/api-extractor": "7.9.11",
        "@microsoft/api-extractor-model": "7.8.19",
        "@microsoft/tsdoc": "0.12.19",
        "@types/http-proxy": "^1.17.4",
        "@types/mdast": "3.0.3",
        "@types/uuid": "8.3.0",
        "chokidar": "3.4.2",
        "colors": "1.4.0",
        "cpy-cli": "3.1.1",
        "del-cli": "3.0.1",
        "fs-extra": "9.0.1",
        "glob": "7.1.6",
        "htmlparser2": "4.1.0",
        "http-proxy": "1.18.1",
        "mdast-util-definitions": "3.0.1",
        "npc": "0.0.1",
        "open": "7.2.1",
        "parcel": "2.0.0-beta.1",
        "preact-render-to-string": "5.1.10",
        "prettier": "2.1.1",
        "remark-parse": "8.0.3",
        "rimraf": "3.0.2",
        "tree-kill": "1.2.2",
        "ts-node": "9.0.0",
        "typescript": "4.0.2",
        "unified": "9.2.0",
        "uuid": "8.3.0"
    }
}
