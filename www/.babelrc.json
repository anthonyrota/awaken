{
    "presets": [
        ["@babel/env", { "modules": false, "bugfixes": true, "loose": true }],
        ["@babel/preset-typescript", { "jsxPragma": "h" }]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-react-jsx", { "pragma": "h" }]
    ]
}
