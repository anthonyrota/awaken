{
    "name": "@awaken/testing",
    "amdName": "awakenTesting",
    "version": "0.0.0",
    "description": "Testing utils for awaken",
    "source": "src/index.ts",
    "main": "dist/awakenTesting.js",
    "module": "dist/awakenTesting.module.js",
    "umd:main": "dist/awakenTesting.umd.js",
    "unpkg": "dist/awakenTesting.umd.js",
    "jsdelivr": "dist/awakenTesting.umd.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "keywords": [
        "awaken",
        "reactive",
        "callbag",
        "callback",
        "observables",
        "observable",
        "iterable",
        "stream",
        "es6",
        "es2015",
        "test",
        "testing",
        "spec"
    ],
    "author": "Anthony Rota",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anthonyrota/awaken.git",
        "directory": "packages/testing"
    },
    "bugs": "https://github.com/anthonyrota/awaken/issues",
    "sideEffects": "false",
    "exports": {
        ".": {
            "browser": "./dist/awakenTesting.module.js",
            "umd": "./dist/awakenTesting.umd.js",
            "import": "./dist/awakenTesting.mjs",
            "require": "./dist/awakenTesting.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "build": "microbundle build --raw --tsconfig ./tsconfig.build.json && api-extractor run --local --verbose && node ./postbuild.js",
        "clean": "rimraf dist && node ../../clearMicrobundleCache.js",
        "clean:prebuild": "rimraf dist"
    },
    "dependencies": {
        "@awaken/core": "^0.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
